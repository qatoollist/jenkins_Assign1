pipeline {
    agent any

    stages {
        stage('Build-on-develop') {
            steps {
                echo 'MVN building...'
            }
        }
        stage('Test-on-develop') {
            steps {
                echo 'JUnit Selenium Testing...'
            }
        }
        stage('Deploy-to-preprod-on-develop') {
            steps {
                echo 'Deploy to preprod...'
            }
        }
        stage('Deploy-to-prod-on-develop') {
            steps {
                echo 'Deploy to Prod...'
            }
        }
    }
}
